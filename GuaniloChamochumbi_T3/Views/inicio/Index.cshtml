@{
    
    
    
    
    
    Layout = null; }
<a href="Auth/Logaut">cerrar sesion</a>
<table class="table table-hover">
    <thead>
        <tr>
            <th>codigo</th>
            <th>Fecha</th>
            <th>Nombre_mascota</th>
            <th>Fecha_nacimiento</th>
            <th>sexo</th>
            <th>especie</th>
            <th>raza</th>
            <th>Tamaño</th>
            <th>datos particulares</th>
            <th>nombre del dueño</th>
            <th>direccion</th>
            <th>telefono</th>
            <th>email</th>

        </tr>

    </thead>
    <tbody>
        @if (User.Identity.IsAuthenticated)
        {
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.Codigo_Registro</td>
        <td>@item.Fecha_registro</td>
        <td>@item.Nombre_mascota</td>
        <td>@item.Fecha_nacimiento</td>
        <td>@item.Id_sexo</td>
        <td>@item.Id_especie</td>
        <td>@item.Id_raza</td>
        <td>@item.Tamaño</td>
        <td>@item.Datos_particulares</td>
        <td>@item.Nombre_dueño</td>
        <td>@item.Direccion</td>
        <td>@item.telefono</td>
        <td>@item.Email</td>

    </tr>}
    }
</table>
<a href="/Inicio/Create" class=" btn-primary">Registrar</a>